{
  "path": "/maps/v3.0/appkeys/{appkey}/coordinates",
  "method": "GET",
  "summary": "Geocoding",
  "description": "Returns coordinate information corresponding to the input address.",
  "parameters": [
    {
      "name": "appkey",
      "in": "path",
      "description": "API key with permission to use this API",
      "required": true,
      "schema": {
        "type": "string"
      }
    },
    {
      "name": "searchCondition",
      "in": "query",
      "description": "Geocoding search conditions",
      "required": true,
      "schema": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "description": "Search keyword",
            "example": "성남시",
            "minLength": 1
          },
          "startposition": {
            "type": "string",
            "default": "0",
            "description": "Search start position. 0 means the first position.",
            "example": 0
          },
          "reqcount": {
            "type": "string",
            "description": "Request count. When set to 0, it will be set to the maximum count of 100",
            "example": 10
          },
          "admcode": {
            "type": "string",
            "description": "Administrative district code",
            "example": 1100000000
          }
        },
        "required": [
          "query"
        ]
      }
    }
  ]
}
